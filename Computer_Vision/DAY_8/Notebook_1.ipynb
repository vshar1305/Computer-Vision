   "source": [
    "img = cv2.imread(r\"D:\\Computer-Vision\\Computer_Vision\\Images\\Ranbir.jpg\")\n",
    "re_img = cv2.resize(img,(400,300))\n",
    "\n",
    "#img[y1:y2,x1:x2]\n",
    "crop = re_img[:200,50:]\n",
    "\n",
    "cv2.imshow(\"Crop_img\",crop)\n",
    "cv2.waitKey(5000)\n",
    "cv2.destroyAllWindows()"
   ]
